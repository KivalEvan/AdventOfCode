{
   "lock": false,
   "fmt": {
      "exclude": ["obj", "bin", "temp", "target", "build"],
      "indentWidth": 3,
      "lineWidth": 100,
      "singleQuote": true
   },
   "imports": { "src/": "./src/langs/ts/" },
   "tasks": {
      "fetch": "deno run --allow-read --allow-write=. --allow-net --allow-env --env ./main/fetchInput.ts",
      "aoc": "deno run --allow-read --allow-run --allow-env --allow-sys ./main/main.ts",
      "aoc:watch": "deno run --watch --allow-read --allow-run --allow-sys --allow-env ./main/main.ts",
      "aoc:bench": "deno run --allow-read --allow-run --allow-env --allow-sys ./main/bench.ts"
   }
}
